{
    "rules": {
        "no-duplicate-tags": {},
        "no-empty-file": {},
        "allowed-tags": {
            "enabled": false,
            "allow": null
        },
        "filename": {
            "style": "snake_case"
        },
        "indentation": {
            "width": 4
        },
        "keyword-order": {
            "tolerateThenBeforeWhen": true
        }
    }
}
