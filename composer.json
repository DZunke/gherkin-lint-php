{
    "name": "dantleech/behat-lint",
    "description": "Behat linter and fixer",
    "type": "project",
    "require": {
        "php": "^8.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^5.0@beta",
        "symfony/console": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/filesystem": "^6.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DTL\\BehatLint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DTL\\BehatLint\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "dan.t.leech@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "symfony/process": "^6.0"
    }
}
